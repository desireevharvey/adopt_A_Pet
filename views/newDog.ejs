<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Adopt-A-Pet â™¡ | Surrender</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet"     integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
    <link rel="stylesheet" href="/styles/main.css" />
</head>
<body>
    <%- include ("./partials/nav.ejs") %>
    <h6><a href="/dogs">Back</a></h6>




    <div class="white-box2">
    <div class="col-md-6 offset-md-3 mt-5">
        <h1>Dog Surrender Form</h1>
        <form action="/dogs/" method="POST" >
          <div class="form-group">
            <label for="exampleInputName">Name</label>
            <input type="text" name="name" class="form-control" id="exampleInputName" placeholder="Enter your dog's name" required="required">
          </div>
          <div class="form-group">
           <label for="exampleInputBreed">Breed</label>
           <input type="text" name="breed" class="form-control" id="exampleInputBreed" placeholder="Enter your dog's breed" required="required">
         </div>
          <div class="form-group">
            <label for="exampleFormControlSelect1">Gender</label>
            <select class="form-control" id="exampleFormControlSelect1" name="gender" required="required">
              <option>Female</option>
              <option>Male</option>
            </select>
          </div>
          <hr>
          <div class="form-group mt-3">
            <label class="mr-2">Enter image URL:</label>
            <input type="text" name="img">
          </div>
          <hr>
          <button type="submit" class="btn btn-primary">Submit</button>
        </form>
    </div> 
    </div>


   <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3" crossorigin="anonymous"></script>
</body>
</html>